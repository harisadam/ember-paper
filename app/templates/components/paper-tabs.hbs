<md-tabs-wrapper>
  <md-tabs-canvas>
    <md-pagination-wrapper>
      {{yield (hash
        tab=(component
          'paper-tab'
          parent=this)
      )}}
      {{#if tabs.length}}
        {{paper-ink-bar}}
      {{/if}}
    </md-pagination-wrapper>
  </md-tabs-canvas>
</md-tabs-wrapper>

{{paper-tabs-content-wrapper parent=this}}
