selectedIdx: {{selected}}<br>
id: {{active.id}}<br>
content: {{active.content}}<br>
content.id: {{active.content.id}}<br>
content.height: {{active.content.height}}<br>
selected.left: {{active.left}}<br>
selected.right: {{active.right}}<br>
<hr>
<md-tabs
  class="md-primary {{if dynamicHeight 'md-dynamic-height'}}"
  style="{{heightStyle}} transition: all 0.5s cubic-bezier(0.35, 0, 0.25, 1)"
  md-align-tabs="{{alignTabs}}">
  {{#paper-tabs-wrapper parent=this}}
    {{yield (hash
      tab=(component
        'paper-tab'
        parent=this)
    )}}
  {{/paper-tabs-wrapper}}
  {{paper-tabs-content-wrapper parent=this}}
</md-tabs>

{{#each tabs as |tab|}}
  <br>object: {{tab}}
  <br>id: {{tab.id}}
  <br>idx: {{tab.index}}
  <br>content {{tab.content}}
  <br>content.id {{tab.content.id}}
  <br>content.height {{tab.content.height}}
  <hr>
{{/each}}
