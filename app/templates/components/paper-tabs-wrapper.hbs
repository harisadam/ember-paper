<md-tabs-canvas>
  <md-pagination-wrapper class="{{if centerTabs 'md-center-tabs'}}" style="width: {{wrapperWidth}}px">
    {{yield}}
    {{#if showInkBar}}
      {{paper-ink-bar
        direction=parent.tabDirection
        leftValue=inkBarLeft
        rightValue=inkBarRight}}
    {{/if}}
  </md-pagination-wrapper>
</md-tabs-canvas>
